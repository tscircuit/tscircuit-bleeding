# Copy this file to .env and fill in your values for local testing
# Note: These are only needed for local testing of uploads
# GitHub Actions uses GitHub Secrets instead

# Cloudflare R2 Configuration
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_R2_ACCESS_KEY_ID=your_access_key_id_here
CLOUDFLARE_R2_SECRET_ACCESS_KEY=your_secret_access_key_here  
CLOUDFLARE_R2_BUCKET_NAME=your_bucket_name_here

# Optional: Custom endpoint URL (usually auto-generated)
# CLOUDFLARE_R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com